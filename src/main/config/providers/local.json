{
  "providerId": "local",
  "displayName": "Local cn-engine",
  "enabled": true,
  "mode": "queued-local",
  "endpoints": [
    {
      "endpointKey": "local.flux2-klein.image",
      "providerModelId": "flux2-klein",
      "displayName": "FLUX.2 Klein (Local)",
      "modes": ["text-to-image", "image-to-image"],
      "outputType": "image",
      "executionMode": "queued-local",
      "requestSchema": {
        "properties": {
          "prompt": { "type": "string", "title": "Prompt" },
          "width": {
            "type": "integer",
            "default": 1024,
            "minimum": 256,
            "maximum": 2048,
            "step": 64
          },
          "height": {
            "type": "integer",
            "default": 1024,
            "minimum": 256,
            "maximum": 2048,
            "step": 64
          },
          "steps": { "type": "integer", "default": 4, "minimum": 1, "maximum": 64 },
          "guidance": {
            "type": "number",
            "default": 3.5,
            "minimum": 1,
            "maximum": 20,
            "step": 0.1
          },
          "sampling_method": { "type": "string", "default": "euler" },
          "ref_image_ids": { "type": "array", "items": { "type": "string" } },
          "ref_image_paths": { "type": "array", "items": { "type": "string" } }
        },
        "required": ["prompt", "width", "height"],
        "order": ["prompt", "width", "height", "steps", "guidance", "sampling_method"]
      }
    }
  ]
}
